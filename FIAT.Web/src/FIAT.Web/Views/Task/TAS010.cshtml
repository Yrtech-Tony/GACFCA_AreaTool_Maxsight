<script type="text/javascript">
        // 将Object的属性值输出成Array
        function objOfValueToArr(object) {
            var arr = [];
            var i = 0;
            for (var item in object) {
                arr[i] = object[item];
                i++;
            }
            return arr;
        }
</script>
