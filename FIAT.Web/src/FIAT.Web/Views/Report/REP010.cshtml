@{
    Layout = "_LayoutMenu";
    ViewData["Title"] = "经销商报告导出";
}
@section HeadCss{
    <style>
        #txtSDate.datewidth, #txtEDate.datewidth {
            width: 48.5%;
        }

        .datemargin {
            margin-left: 3%;
            margin-top: -1.4em;
        }

        .gap-icon {
            margin-right: 5px;
        }

        .jqgridbuttonwidth {
            width: 90%;
        }

        .decoration {
            background-image: url('/images/x.gif');
            background-repeat: no-repeat;
            font-style: italic;
            background-size: 100% 100%;
        }

        .dotdotdot {
            display: inline-block;
            white-space: nowrap;
            overflow: hidden;
            text-overflow: ellipsis;
            width: 260px;
            line-height: 20px;
        }
    </style>
}
@section Scripts{
    <script src="~/lib/crypto1/crypto/crypto.js"></script>
    <script src="~/lib/crypto1/hmac/hmac.js"></script>
    <script src="~/lib/crypto1/sha1/sha1.js"></script>
    <script src="~/lib/base64.js"></script>
    <script src="~/lib/plupload-2.1.9/js/plupload.full.min.js"></script>
    <script src="~/js/upload.js"></script>
    <script src="~/lib/lightbox2/js/lightbox-2.6.min.js"></script>
    <script src="~/lib/spin.js"></script>
    <link href="~/lib/lightbox2/css/lightbox.css" rel="stylesheet" />
    <!-- 1. Load -->
    <script type="text/javascript">
        var _grid, _gridName;
        var _execute;
        $(document).ready(function () {
            fn_Initial();
            fn_InitialData();
            fn_SetButtonStatus();
            fn_GridInit1();
            if (_WAPYN) {
                $("#grid1").setGridParam().hideCol("Rate").trigger("reloadGrid"); //Wap版不显示下载按钮
            }
            //拜访类型
            fn_SetVType();

            $("#selVisitType").on("change", function () {
                fn_getPlans();
                if ($("#selVisitType").val() == 'B') {
                    $.sa.warning("客服区域巡检中:道路救援响应率、召回完成率系统权重为0%,总部统一打分，现场巡检总分为79分");
                } else if ($("#selVisitType").val() == 'A') {
                    var bizType = "@Context.Request.Cookies[SessionKeys.SESSION_ORGBIZTYPE]";
                    if (bizType == "S" ) {
                        $.sa.warning("销售明检体系检核总得分为 77分");
                    }
                    else if (bizType == "A") {
                        $.sa.warning("客户服务明检体系检核中：培训管理系统权重为0%，总部统一打分，现场检核总分为 98分");
                    }
                }
            });

            $("#selPlanLst").on("change", function () {
                fn_SetServerData();
            });
        });
    </script>
    <!-- 2. Init -->
    <script type="text/javascript">
        function fn_Initial() {
            _gridName = 'grid';
            _grid = $('#' + _gridName);
            $(".form_datetime").datetimepicker({
                format: 'yyyy-mm-dd',
                language: 'zh-CN',
                minView: 2, //显示到具体日期，如果设置到1，则显示到具体时间点。
                autoclose: 1,
                todayHighlight: 1,
                todayBtn: 1,
                startView: 2, //开始页面设置为具体到日期的那个页面。
                pickerPosition: 'bottom-left'
            });
            $("#txtSDate").val("@ViewBag.FirstDay");
            $("#txtEDate").val("@ViewBag.CurrentDate");
            DateFormatCheckEvent("txtSDate", { CheckType: "BLANK" });
            DateFormatCheckEvent("txtEDate", { CheckType: "BLANK" });

            $('#btnSearch').on('click', function () {
                if (fn_SearchChk()) {
                    if ($("#selSourceType").val() == "D") {
                        fn_Search();
                    } else {
                        fn_SearchData();
                    }

                }

            });

            $("#btnSave").on("click", function () {
                fn_Save();
            });
            $("#btnDelete").on("click", function () {
                fn_Delrow();
            });
            $("#selSourceType").on("change", function () {
                fn_changeGrid();
            })
            $("#btnTourData").on("click", function () {
                fn_downloadExcel();
            })

        }
    </script>
    <!-- 3. InitData -->
    <script type="text/javascript">
        function fn_InitialData() {
            $.sa.get(TASKGETSTATUS, { GroupCode: '15' },
                {
                    "success": function (data) {
                        _execute = NameValueListToJson('Name', 'Value', JSON.parse(data.body), { 'SelectType': 'BLANK' });
                        $("#selSourceType").html(JsonToCboList("Name", "Value", JSON.parse(data.body), {}));
                        $("#selSourceType").val("D");
                        fn_GridInit();
                        if ($("#selSourceType").val() == "D") {
                            $("#btnSave").addClass("hidden");
                            $("#btnDelete").addClass("hidden");
                            $("#divUpload").addClass("hidden");
                            $("#distributor_id").removeClass("hidden");
                            //$("#select_distributor_id").val("0");
                            $("#divgrid").addClass("hidden");
                            $("#divgrid1").removeClass("hidden");
                        } else {
                            if ('@Context.Request.Cookies[SessionKeys.SESSION_USERTYPE]' == "A" || '@Context.Request.Cookies[SessionKeys.SESSION_USERTYPE]' == "R") {
                                $("#btnSave").removeClass("hidden");
                                $("#btnDelete").removeClass("hidden");
                                $("#divUpload").removeClass("hidden");
                            }
                            $("#distributor_id").addClass("hidden");
                            //$("#select_distributor_id").val("0");
                            $("#divgrid1").addClass("hidden");
                            $("#divgrid").removeClass("hidden");
                        }
                    },
                    "failure": function (data, statusText, jqXHR) {

                    }
                }
                , $(".Grid")
                , { type: "spin" });

            @*$.sa.get(SEARCHNOTIFIDIS, { UserId: '@Context.Request.Cookies[SessionKeys.SESSION_USERID]' },
                             {
                                 "success": function (data) {
                                     $("#select_distributor_id").html(JsonToCboList("DisName", "DisId", eval(data.body), { 'SelectType': 'ALL' }));

                                 },
                                 "failure": function (data, statusText, jqXHR) {
                                     $("#callback_1_layer").html(data.msg);
                                 }
                             }
                             , $(".Grid")
                             , { type: "spin" });*@
        }

        //查询检核类型
        function fn_SetVType() {
            $.sa.get(TASKGETSTATUS,
                         { GroupCode: '09', BusId: "@Context.Request.Cookies[SessionKeys.SESSION_ORGBIZID]" },
                         {
                             "success": function (data) {
                                 if (data != null) {
                                     var jData = JSON.parse(data.body);
                                     if (jData.length == 1) {
                                         $('#selVisitType').html(JsonToCboList("Name", "Value", JSON.parse(data.body)));
                                         $("#selVisitType").prop("disabled", true);
                                         if ($("#selVisitType").val() == 'B') {
                                             $.sa.warning("客服区域巡检中:道路救援响应率、召回完成率系统权重为0%,总部统一打分，现场巡检总分为79分");
                                         } else if ($("#selVisitType").val() == 'A') {
                                             var bizType = "@Context.Request.Cookies[SessionKeys.SESSION_ORGBIZTYPE]";
                                             if (bizType == "S") {
                                                 $.sa.warning("销售明检体系检核总得分为 77分");
                                             }
                                             else if(bizType=="A") {
                                                 $.sa.warning("客户服务明检体系检核中：培训管理系统权重为0%，总部统一打分，现场检核总分为 98分");
                                             }
                                         }
                                     }
                                     else {
                                         $('#selVisitType').html(JsonToCboList("Name", "Value", JSON.parse(data.body), { "SelectType": "ALL" }));
                                         $("#selVisitType").prop("disabled", false);
                                     }
                                     fn_getPlans();
                                 }
                             },
                             "failure": function (data, statusText, jqXHR) {
                                 $.sa.warning(data.msg);
                             }
                         });
        }
        //巡检月份
        function fn_getPlans() {
            $.sa.get(GETPLANSBYORGANIZATION
               , {
                   busId: "@Context.Request.Cookies[SessionKeys.SESSION_USERTYPE]" === "A" ? "0" : "@Context.Request.Cookies[SessionKeys.SESSION_ORGBIZID]"
                   , repId: "@Context.Request.Cookies[SessionKeys.SESSION_ORGREPID]" === "" ? "0" : "@Context.Request.Cookies[SessionKeys.SESSION_ORGREPID]"
                   , areaId: "@Context.Request.Cookies[SessionKeys.SESSION_ORGZIONID]" === "" ? "0" : "@Context.Request.Cookies[SessionKeys.SESSION_ORGZIONID]"
                   , zoneId: "@Context.Request.Cookies[SessionKeys.SESSION_ORGAREAID]" === "" ? "0" : "@Context.Request.Cookies[SessionKeys.SESSION_ORGAREAID]"
                   , vType: $.trim($("#selVisitType").val())
                   , status: "A"
               },
             {
                 "success": function (data) {
                     var result = JSON.parse(data.body);
                     $("#selPlanLst").html(JsonToCboList("Name", "Value", result, { 'SelectType': 'SELECT' }));
                     fn_SetServerData();
                 },
                 "failure": function (data, statusText, jqXHR) {
                 }
             });
        }

        function fn_SetServerData() {
            if ($("#selPlanLst").val() === "select") {
                $("#selServerLst").html(JsonToCboList("DisName", "DisId", "", { 'SelectType': 'All' }));
                return;
            }
            $.sa.get(GETTOURDISTRIBUTORLIST,
              // 'http://localhost:6502/fiat/api/v1/Tour/GetTourDistributorList',
                           {
                               Batch: $("#selPlanLst").val()
                               , UserId: "@Context.Request.Cookies[SessionKeys.SESSION_USERID]"
                               , disCode: '%'
                               , disName: '%'
                               //, searchType: "Z" Modify by dong.limin 2017年5月18日10:54:25  报告详情查看页面使用
                                , searchType: "S"
                           },
                           {
                               "success": function (data) {
                                   if (data != null && JSON.parse(data.body).length > 0) {
                                       $("#selServerLst").html(JsonToCboList("DisName", "DisId", JSON.parse(data.body), { 'SelectType': 'All' }));
                                   }
                               },
                               "failure": function (data, statusText, jqXHR) {
                               }
                           });
        }
    </script>

    <!-- 4. Custom Function -->
    <script type="text/javascript">
        function fn_Save() {
            if (!fn_ValidateBeforeSave()) {
                return;
            }
            var editData = _grid.getChangedCells('all');
            var attachList = [];
            for (var i = 0; i < editData.length; i++) {
                var data = { AttachName: editData[i].AttachName, Code: editData[i].Code, Url: editData[i].Url, DisCode: editData[i].DisCode, GRUD: editData[i].GRUD, Id: editData[i].Id == "" ? 0 : editData[i].Id };
                attachList.push(data);
            }
            var saveData = { AttachList: attachList, UserId: "@Context.Request.Cookies[SessionKeys.SESSION_USERID]" };
            //$.sa.post("http://10.202.101.144:6505/fiat/api/v1/Report/SaveReportAttachment",
            $.sa.post(SAVEREPORTATTACHMENT,
                 JSON.stringify(saveData),
                 {
                     "success": function (data) {
                         $.sa.alert('保存成功！');
                         fn_SearchData();
                     },
                     "failure": function (data) {
                         $.sa.alert("保存失败");
                     }
                 }, $("body"));
        }

        function fn_PreviewFile(cellvalue, options, rowObject) {
            var previewPath = rowObject.Url;
            if (rowObject.AttachName.substr(rowObject.AttachName.lastIndexOf(".")) == ".gif" || rowObject.AttachName.substr(rowObject.AttachName.lastIndexOf(".")) == ".jpg" || rowObject.AttachName.substr(rowObject.AttachName.lastIndexOf(".")) == ".png")
                return "<a href='" + previewPath + "' data-lightbox='PreviewPicture'   style='margin-left: 15px; cursor: pointer; text-decoration: underline;' onclick='fn_DownLoadCnt(" + rowObject.Id + ")'>" + rowObject.AttachName + "</a>";
            else {
                var url = '@Url.Action("DownLoadForRename", "Report")' + "?fileName=" + encodeURIComponent(rowObject.AttachName) + "&sourcepath=" + previewPath;
                return " <a href='" + url + "'  class='btn dotdotdot' style='text-align:left;' download='" + rowObject.AttachName + "'onclick='fn_DownLoadCnt(" + rowObject.Id + ")'>" + rowObject.AttachName + "</a>";
            }
        }

        function fn_UnPreviewFile(cellvalue, options, rowObject) {
            return cellvalue;
        }
        function fn_ValidateBeforeSave() {
            fn_SaveCell.call(_grid);
            var editData = _grid.getChangedCells('all');
            if (editData.length == 0) {
                $.sa.warning("没有要保存的数据");
                return false;
            }
            for (var i = 0; i < editData.length; i++) {
                if ((editData[i].GRUD == "I" || editData[i].GRUD == "R") && editData[i].Code == "") {
                    $.sa.warning("请选择检核类别");
                    return false;
                }
            }
            return true;
        }
        function fn_DownLoadCnt(id) {
            //$.sa.post("http://10.202.101.144:6505/fiat/api/v1/Report/UpdateAttachmentDownloadCnt",
            $.sa.post(UPDATEATTACHMENTDOWNLOADCNT,
                JSON.stringify(id),
               {
                   "success": function (data) {
                       fn_SearchData();
                   },
                   "failure": function (data) {
                       $.sa.alert("保存失败");
                   }
               }, $("body"));

        }
        function fn_SetButtonStatus() {
            var userType = '@Context.Request.Cookies[SessionKeys.SESSION_USERTYPE]';
            if (userType == "A" || userType == "R") {
                $("#btnSave").removeClass("hidden");
                $("#btnDelete").removeClass("hidden");
                $("#divUpload").removeClass("hidden");
            } else {
                $("#btnSave").addClass("hidden");
                $("#btnDelete").addClass("hidden");
                $("#divUpload").addClass("hidden");
            }
        }
        function fn_SearchChk() {
            if ($("#txtSDate").val() > $("#txtEDate").val()) {
                $.sa.warning("结束日期不能小于开始日期"
            , {
                fnClose: function () {
                    $("#txtEDate").focus();
                    return false;
                }
            });
            } else {
                return true;
            }
        }
        function fn_SearchData() {
            //$.sa.get("http://10.202.101.144:6505/fiat/api/v1/Report/GetAttachmentByUserId",
            $.sa.get(GETATTACHMENTBYUSERID,
                {
                    'sourceType': $("#selSourceType").val()
                , 'sDate': $("#txtSDate").val().replace(/\-/gi, '')
                , 'eDate': $("#txtEDate").val().replace(/\-/gi, '')
                , 'userId': '@Context.Request.Cookies[SessionKeys.SESSION_USERID]'
                },
             {
                 "success": function (data) {
                     fn_SetGrid(JSON.parse(data.body));
                     if (JSON.parse(data.body).length == 0) {
                         $.sa.alert(data.msg);
                     }
                 },
                 "failure": function (data, statusText, jqXHR) {
                     $.sa.warning(data.msg);
                 }
             }
            , $("body"));
        }
        function fn_Search() {
            //var sdate = $("#txtSDate").val().replace(/\-/gi, '');
            //var edate = $("#txtEDate").val().replace(/\-/gi, '');

            var batch = $("#selPlanLst").val();
            var disId = $("#selServerLst").val();
            if (batch == null || batch == "select") {
                $.sa.warning("请选择巡检月份!");
                $("#selPlanLst").focus();
                return;
            }

            //$.sa.get('http://localhost:6502/fiat/api/v1/Report/GetPlansListForExcelDownload',
            $.sa.get(GETPLANSLISTFOREXCELDOWNLOAD,
                          { UserId: '@Context.Request.Cookies[SessionKeys.SESSION_USERID]', DisId: disId, Batch: batch },
                          {
                              "success": function (data) {
                                  if (data != null && JSON.parse(data.body).length > 0) {
                                      //$('#selStatus').html(JsonToCboList("Name", "Code", JSON.parse(data.body), { "SelectType": "ALL" }));
                                      fn_SetGrid1(JSON.parse(data.body));
                                  } else {
                                      $.sa.warning("没有查询到数据！");
                                      fn_SetGrid1("");
                                  }
                              },
                              "failure": function (data, statusText, jqXHR) {
                                  fn_SetGrid1("");
                                  $.sa.warning(data.msg);
                              }
                          }, $("body"));
        }


        function fn_Delrow() {
            var ids = _grid.jqGrid('getGridParam', 'selarrrow');
            if (ids.length <= 0) {
                $.sa.warning("请选择要删除的行。", {
                    fnClose: function () {
                    }
                });
                return;
            }
            for (var i = ids.length - 1; i >= 0; i--) {
                if (_grid.getCell(ids[i], "GRUD") != "I") {

                    _grid.setCell(ids[i], "GRUD", "D");
                    $(_grid.getGridRowById(ids[i])).addClass("decoration");
                    $(_grid.getGridRowById(ids[i])).addClass("edited");
                }
                else {
                    _grid.delRowData(ids[i]);
                }
            }
        }
        function fn_changeGrid() {
            var selSourceType = $("#selSourceType").val();
            if (selSourceType == "D") {
                $("#btnSave").addClass("hidden");
                $("#divUpload").addClass("hidden");
                $("#btnDelete").addClass("hidden");
                //_grid1.trigger('reloadGrid');
                $("#divgrid").addClass("hidden");
                $("#divgrid1").removeClass("hidden");
                $("#distributor_id").removeClass("hidden");
                //$("#select_distributor_id").val("0");
                fn_GridInit1();
                fn_SetGrid1();
            } else {
                if ('@Context.Request.Cookies[SessionKeys.SESSION_USERTYPE]' == "A" || '@Context.Request.Cookies[SessionKeys.SESSION_USERTYPE]' == "R") {
                    $("#btnSave").removeClass("hidden");
                    $("#btnDelete").removeClass("hidden");
                    $("#divUpload").removeClass("hidden");
                }
                //_grid.trigger('reloadGrid');
                $("#divgrid1").addClass("hidden");
                $("#divgrid").removeClass("hidden");
                $("#distributor_id").addClass("hidden");
                //$("#select_distributor_id").val("0");
                fn_SetGrid();
            }
        }
        function fn_ExcelDownLoad(pid, id, name, visitTypeName, visitDateTime) {
            var disid = id;
            var disname = name;
            var name = Reconvert("@Context.Request.Cookies[SessionKeys.SESSION_NAME]");

            var opts = {
                type: "Image",
                imageUrl: "/images/ajax-loader.gif",
                lines: 10, // loading小块的数量
                length: 0, // 小块的长度
                width: 18, // 小块的宽度
                radius: 40, // 整个圆形的半径
                corners: 1, // 小块的圆角，越大则越圆
                rotate: 10, // loading动画的旋转度数，貌似没什么实际作用
                color: '#047EC4', // 颜色
                speed: 1, // 变换速度
                trail: 60, // 余晖的百分比
                shadow: false, // 是否渲染出阴影
                hwaccel: false, // 是否启用硬件加速
                className: 'spinner', // 给loading添加的css样式名
                zIndex: 2e9, // The z-index (defaults to 2000000000)
                top: '50%', // Top position relative to parent in px
                left: '50%' // Left position relative to parent in px
            };
            var spinner = new Spinner(opts).spin(document.getElementById('loadBar'));

            $.ajax({
                type: "POST",
                url: "/Report/UploadScoreAjaxDown?disCode=" + disid + "&startTime=" + $("#txtSDate").val() + "&endTime=" + $("#txtEDate").val()
                        + "&status=F&pid=" + pid + "&disName=" + disname + "&name=" + name
                        + "&visitTypeName=" + visitTypeName + "&visitDateTime=" + visitDateTime + "&visitType=" + $("#selVisitType").val() + "&batch=" + $("#selPlanLst").val(),
                contentType: false,
                processData: false,
                //data: data,
                success: function (message) {
                    document.getElementById("a").href = "/Template/" + message.split('\\')[message.split('\\').length - 1];
                    document.getElementById("a").click();
                },
                beforeSend: function () {
                    $("#modal-container-loadbar").css("display", "block");
                    spinner.spin(document.getElementById('loadBar'));
                },
                complete: function () {
                    spinner.stop();
                    $("#modal-container-loadbar").css("display", "none");
                },
                error: function () {
                    $.sa.warning("下载失败,请重试.");
                }
            });
        }
    </script>
    <!--Grid-->
    <script type="text/javascript">
        function fn_GridInit() {
            _grid.jqGrid({
                datatype: 'local',
                colModel: [

                    {
                        name: 'AttachName', label: '附件名', width: 330, align: 'left'
                        , formatter: fn_PreviewFile,
                        unformat: fn_UnPreviewFile
                        , cellattr: function (rowId, val, rawObject, cm, rdata) {
                            return 'class="readonlycell"';
                        }
                    }
                    ,
                    {
                        name: 'Code', label: '检核类别', width: 130, align: 'center', editable: true, edittype: 'select', hidden: true
                        , formatter: 'select',
                        formatoptions: {
                            value: _execute
                        },
                        editoptions: {
                            'class': 'form-control',
                            value: _execute
                        }
                    },
                    {
                        name: 'DisCode', label: '小区/经销商代码', width: 130, align: 'center',
                        cellattr: function (rowId, val, rawObject, cm, rdata) {
                            return 'class="readonlycell"';
                        }
                    },
                    {
                        name: 'DisName', label: '小区/经销商名', width: 250, align: 'left',
                        cellattr: function (rowId, val, rawObject, cm, rdata) {
                            return 'class="readonlycell"';
                        }
                    },
                    {
                        name: 'InDateTime', label: '上传日期', width: 135, align: 'center',
                        cellattr: function (rowId, val, rawObject, cm, rdata) {
                            return 'class="readonlycell"';
                        }
                    },
                    {
                        name: 'DownloadCnt', label: '下载次数', width: 80, align: 'center',
                        cellattr: function (rowId, val, rawObject, cm, rdata) {
                            return 'class="readonlycell"';
                        }
                    },
                    {
                        name: 'Id', label: 'Id', width: 40, align: 'center', hidden: true

                    },
                    {
                        name: 'GRUD', label: 'GRUD', width: 40, align: 'center', hidden: true

                    },
                    {
                        name: 'Url', label: 'Url', width: 40, align: 'center', hidden: true

                    }
                ],
                width: $('#Grid').width(),
                shrinkToFit: false,
                height: 'auto',
                rownumbers: true,
                rownumWidth: 50,
                rowNum: 15,
                rowList: [15, 30, 45],
                footerrow: false,
                cellsubmit: 'clientArray',
                pager: jQuery('#pager'),
                cellEdit: true,
                editurl: 'clientArray',
                viewrecords: true,
                multiselect: true,
                afterInsertRow: function (rowid, r) {
                },
                gridComplete: function (id) {
                    var rowNum = parseInt($(this).getGridParam("records"), 50);
                    if (rowNum > 0) {
                        $(".ui-jqgrid-sdiv").show();
                    }
                    else {
                        $(".ui-jqgrid-sdiv").hide();
                    }
                    $('#pager_left').css('width', '25%');
                    $('#pager_right').css('width', '25%');

                    $("[aria-describedby='grid_rn']").addClass("readonlycell");
                }
                , onCellSelect: function (rowid, iCol, cellcontent, e) {
                    var userType = '@Context.Request.Cookies[SessionKeys.SESSION_USERTYPE]';
                    if (userType == "A" || userType == "R") {
                        $("#grid").setColProp('Code', { 'editable': true });
                    } else {
                        $("#grid").setColProp('Code', { 'editable': false });
                    }
                },
                afterInsertRow: function (rowid, r) {
                    var userType = '@Context.Request.Cookies[SessionKeys.SESSION_USERTYPE]';
                    var $t = $(this);
                    if (userType == "A" || userType == "R") {
                        $t.find('#' + rowid).find("[aria-describedby='grid_Code']").removeClass("readonlycell");
                    } else {
                        $t.find('#' + rowid).find("[aria-describedby='grid_Code']").addClass("readonlycell");
                    }

                }
            });
        }
        function fn_SetGrid(mydata) {
            mydata = mydata || [];
            _grid.clearGridData()
            .setGridParam({
                data: mydata
            }).trigger('reloadGrid');
        }
