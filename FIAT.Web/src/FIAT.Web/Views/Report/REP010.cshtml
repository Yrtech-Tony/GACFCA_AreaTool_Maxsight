    <script type="text/javascript">
        function fn_downloadExcel() {
            var batch = $("#selPlanLst").val();
            var disId = $("#selServerLst").val();
            if (batch == null || batch == "select") {
                $.sa.warning("请选择巡检月份!");
                $("#selPlanLst").focus();
                return;
            }
            $.sa.get(GETTOURBASESCOREBYDISID,
                          { UserId: '@Context.Request.Cookies[SessionKeys.SESSION_USERID]', DisId: disId, Batch: batch },
                          {
                              "success": function (data) {
                                  if (data != null) {
                                      var rData = JSON.parse(data.body);
                                      if (rData.touList.length > 0) {
                                          var rJson = [];
                                          rJson.push(JSON.parse(rData.colList));
                                          rJson.push(JSON.parse(rData.wList)[0]);
                                          $.each(JSON.parse(rData.touList), function (x, obj) {
                                              rJson.push(obj);
                                          })
                                          var cJson = [];
                                          cJson.push(JSON.parse(rData.bList));
                                          cJson.push(JSON.parse(rData.hList));
                                          $.each(JSON.parse(rData.rList), function (x, obj) {
                                              cJson.push(obj);
                                          })
                                          fn_createExcel(rJson,cJson);
                                      } else {
                                          $.sa.warning("没有可下载的数据！");
                                      }
                                  } else {
                                      $.sa.warning("没有可下载的数据！");
                                  }
                              },
                              "failure": function (data, statusText, jqXHR) {
                                  $.sa.warning(data.msg);
                              }
                          }, $("body"));

        }
        function fn_createExcel(data1,data2) {
            var file = new xlsx.File();

            var sheet1 = file.addSheet("审计项目得分");
            sheet1.setOrientation('landscape');
            fn_setsheet1Header(sheet1, data1);

            var sheet2 = file.addSheet("检查标准得分");
            sheet2.setOrientation('landscape');
            fn_setsheet2Header(sheet2, data2);

            //for (var i = 0; i < 4; i++) {
            //    if (i % 2 == 0)
            //        sheet1.col(i).width = 10;
            //    else
            //        sheet1.col(i).width = 12;
            //}
            var now = "数据报告" + ".xlsx";
            file
              .saveAs('blob')
              .then(function (content) {
                  saveAs(content, now);
              });

        }
        function fn_setsheet1Header(sheet, data) {           
            for (var x = 0; x < 2; x++) {
                var h = sheet.addRow();
                h.isCustom = false;
                var hd = objOfValueToArr(data[x]);
                hd.unshift("大区代码", "大区名称", "区域代码", "区域名称", "小区代码", "小区名称");
                for(var i=0;i<8;i++){
                    var c1 = setCellHeader_o(h, hd[i]);
                    if (x == 0) {
                        c1.vMerge = 1;
                    }                
                }
               
                for (var y = 8; y < hd.length; y++) {
                    setCellHeader_o(h, y==8? x==0?"审计项目":"权重":hd[y]);
                }

            }

            for (var x = 2; x < data.length; x++) {
                var r = sheet.addRow();
                r.isCustom = false;
                var rowd=objOfValueToArr(data[x])
                for(var y=0;y<rowd.length;y++){
                    setCellData_o(r,y==8? "得分" : rowd[y], 'center');
                }
            }
        }
        function fn_setsheet2Header(sheet, data) {
            for (var x = 0; x < 2; x++) {
                var h = sheet.addRow();
                h.isCustom = false;
                var hd = objOfValueToArr(data[x]);
                hd.unshift("大区代码", "大区名称", "区域代码", "区域名称", "小区代码", "小区名称");
                for (var i = 0; i < 8; i++) {
                    var c1 = setCellHeader_o(h, hd[i]);
                    if (x == 0) {
                        c1.vMerge = 1;
                    }
                }

                for (var y = 8; y < hd.length; y++) {
                    setCellHeader_o(h, y == 8 ? x == 0 ? "审计项目" : "检查标准" : hd[y]);
                }
            }

            for (var x = 2; x < data.length; x++) {
                var r = sheet.addRow();
                r.isCustom = false;
                var rowd = objOfValueToArr(data[x])
                for (var y = 0; y < rowd.length; y++) {
                    setCellData_o(r, y == 8 ? "得分" : rowd[y], 'center');
                }
            }
            //sheet.setColWidth(0, objOfValueToArr(data[0]).length - 1, 20);
        }
    </script>
